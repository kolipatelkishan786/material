{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "lib": [
            "ES2020"
        ],
        "rootDir": "./",
        "moduleResolution": "Node",
        "baseUrl": "./",
        "paths": {
            "*": [
                "node_modules/*",
                "am-types/*",
                "src/admin/types/*"
            ]
        },
        "pretty": true,
        "skipLibCheck": true,
        "preserveSymlinks": true
    }
}